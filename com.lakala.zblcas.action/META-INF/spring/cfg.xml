<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:a="http://www.lakala.com/schema/amber-core" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">


	<beans profile="default">
		<context:property-placeholder location="META-INF/cfg/dev.properties"/>
	</beans>
	<beans profile="dev">
		<context:property-placeholder location="META-INF/cfg/dev.properties"/>
	</beans>
	<beans profile="test">
		<context:property-placeholder location="META-INF/cfg/test.properties"/>
	</beans>
	<beans profile="prod">
		<context:property-placeholder location="META-INF/cfg/prod.properties"/>
	</beans>

</beans>